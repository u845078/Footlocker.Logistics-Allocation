@model Footlocker.Logistics.Allocation.Models.ReInitializeSKUModel
@{
    ViewBag.Title = "CreateReInitializeSKU";
}

<script></script>

<h2>Create Re-Initialize SKU</h2>

@using (Html.BeginForm())
{
    @Html.ValidationSummary(true)
    if ((ViewData["message"] != "") && (ViewData["message"] != null))
    {
        <font color="red">@Html.Raw(ViewData["message"])</font>
    }

    <fieldset>
        <legend>Re-Initialize SKU</legend>

        <div class="editor-label">
            @Html.LabelFor(model => model.SKU, "SKU") (##-##-#####-##)
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.SKU)
            @Html.ValidationMessageFor(model => model.SKU)
        </div>
        <p>
            <input type="submit" value="Create" class="t-button" />
        </p>
    </fieldset>
}

<div>
    @Html.ActionLink("Back to List", "ReInitializeSKU", null, new { @class = "t-button" })
</div>

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}